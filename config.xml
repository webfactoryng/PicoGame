<?xml version='1.0' encoding='utf-8'?>
<widget id="com.picoapp.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>picoapp</name>
    <description>
        Arrange the image
    </description>
    <author email="support@webfactory.ng" href="http://webfactory.ng">
        Webfactory Team
    </author>
    <content src="index.html" />
	<icon src="icon.png" />

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
